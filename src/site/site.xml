<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/DECORATION/1.7.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://maven.apache.org/DECORATION/1.7.0 http://maven.apache.org/xsd/decoration-1.7.0.xsd" name="JaStaCry">
    <skin>
        <groupId>org.apache.maven.skins</groupId>
        <artifactId>maven-fluido-skin</artifactId>
        <version>1.5</version>
    </skin>

    <bannerLeft>
        <name>JaStaCry - Java Stacked Crypto</name>
    </bannerLeft>
    <bannerRight>
        <src>img/logo150_0.png</src>
        <href>${project.url}</href>
    </bannerRight>

    <body>

        <links>
            <item href="index.html" name="JaStaCry" />
            <item href="https://www.jastacry.org/" name="Website" />
            <item href="https://testlink.myocastor.de" name="TestLink" />
            <item href="https://jen.myocastor.de" name="Jenkins" />
        </links>

        <breadcrumbs>
            <item name="JaStaCry Doku" href="http://site.jastacry.org/index.html" />
        </breadcrumbs>

        <menu ref="parent" inherit="top">
            <item href="index.html" name="JaStaCry Home" />
        </menu>
        <menu ref="modules" inherit="top" />
        <menu name="User guide">
            <item href="readme.html" name="Readme" />
        </menu>
        <menu ref="reports" inherit="bottom" />

    </body>

    <custom>
        <fluidoSkin>
            <sourceLineNumbersEnabled>true</sourceLineNumbersEnabled>
            <topBarEnabled>true</topBarEnabled>
            <sideBarEnabled>true</sideBarEnabled>
        </fluidoSkin>
    </custom>
</project>
